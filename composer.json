{
    "name": "mchuluq/larv-rbac",
    "description": "A Laravel Group-Role Based Access Control.",
    "keywords": [
        "laravel",
        "session",
        "rbac"
    ],
    "support": {
        "issues": "https://github.com/mchuluq/larv-rbac/issues",
        "source": "https://github.com/mchuluq/larv-rbac"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Mochammad C. Chuluq",
            "email": "mchuluq@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0",
        "illuminate/database": "^8.0",
        "illuminate/http": "^8.0",
        "illuminate/auth": "^8.0",
        "illuminate/contracts": "^8.0",
        "illuminate/cookie": "^8.0",
        "illuminate/encryption": "^8.0",
        "illuminate/mail": "^8.0",
        "illuminate/routing": "^8.0",
        "illuminate/session": "^8.0",
        "illuminate/validation": "^8.0",
        "matomo/device-detector": "6.4.8"
    },
    "require-dev": {
        "mockery/mockery": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "Mchuluq\\Larv\\Rbac\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mchuluq\\Larv\\Rbac\\RbacServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
